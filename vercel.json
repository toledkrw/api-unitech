{
  "version": 2,
  "build": {
    "env": {
      "POETRY_VERSION": "1.8.2"
    }
  },
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10",
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/app.py",
      "methods": ["GET","POST","PUT","DELETE","OPTIONS"],
      "headers": {
        "Acces-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "*",
        "Access-Control-Allow-Methods": "*"
      }
    }
  ]
}
